# Copy to .env and fill in the required values.
# APP_TOKEN: Secret token for backend API requests.
APP_TOKEN=your-app-token

# PUBLIC_APP_TOKEN: Exposed token used by the client-side application.
PUBLIC_APP_TOKEN=your-public-app-token

# PUBLIC_APP_MODE: Mode for the application (e.g., 'development', 'production').
PUBLIC_APP_MODE=development
